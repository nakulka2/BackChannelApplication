<img width="100%" src="../images/banner1.png"  border=0 />
 <h1>Listing users</h1>

<table border="1">
<tr>
<th>Username</th>
<th>Email</th>
<th>Type</th>
<th>Action</th>
</tr>

<% @users.each do |user| %>
<tr>
<td><%= user.username %></td>
<td><%= user.email %></td>
<td><%= user.user_type %></td>
<td><%= link_to 'Show', user %>
<%= link_to 'Destroy', :id=>user.id,:confirm=> 'Are you sure?',:action =>'destroy' %></td>
</tr>
<% end %>
</table>

